  "onClick": "{{\n  usuario_login.run(\n    () => {\n      const u = usuario_login.data[0];\n\n      if (!u) {\n        showAlert(\"Usuário ou senha inválidos\", \"error\");\n        return;\n      }\n\n      storeValue(\"user\", u);\n      storeValue(\"roles\", u.roles || []);\n      storeValue(\"filialId\", u.id_filial);\n      storeValue(\"isAdmin\", (u.roles || []).includes(\"ADMIN\"));\n\n      navigateTo(\"Cadastro Usuario\");\n    },\n    (err) => {\n      showAlert(\"Erro ao fazer login\", \"error\");\n      console.log(err);\n    }\n  );\n}}\n",
